{
  "name": "api-oracle-nestjs",
  "version": "1.0.0",
  "description": "API Oracle Database",
  "author": "Filipe Campos",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node dist/src/main",
    "dev": "SET ENV=local&&SET PORT=3000&&nest start --watch",
    "hml": "SET ENV=local&&SET PORT=3000&&nest start --watch",
    "debug": "SET ENV=local&&SET PORT=3000&&nest start --watch --debug",
    "prod": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "SET ENV=local&&jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:file": "SET ENV=local&&jest -i src/modules/user/repositories/user.repository.spec.ts --detectOpenHandles --forceExit",
    "test:lib": "SET ENV=local&&jest -i libs/marvel/src/marvel.service.spec.ts --forceExit"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "newrelic": "^8.14.1",
    "oracledb": "^5.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/oracledb": "^5.2.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filipercampos/api-oracle-nestjs"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "users",
    "oracle"
  ]
}